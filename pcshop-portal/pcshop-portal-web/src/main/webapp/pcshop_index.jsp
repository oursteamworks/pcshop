  <script type="text/javascript">



    //顶通关闭并删除开始
    function l_adtop_1(obj,btn_close){
      var close = btn_close || ".layer-close";
      $(obj).on('click',close,function(){
        var $parent = $(this).closest(obj);
        $parent.slideUp(500,function(){
          $(this).remove()
        });
      })
    }
    //超级IP馆幻灯动效
    function le_slide_4(obj) {
      var $box = $(obj.box); //最外层的div
      var $pics = $box[0] ? $box.find(obj.pics) : $(obj.pics); //幻灯模块
      var $small_list = $box.find(obj.small_list); //幻灯模块下面的小图列表
      var speed = obj.speed || 3000//播放速度

      //播放动画时限制点击
      var space = obj.space ? (obj.space>=500? obj.space : 500) : 500;
      $pics.open=1;//1可以点击，0不可以点击；


      var $lis = $pics.find(".listpic li");
      //不够5条的补上条数，最少3条数据才可以
      // $box.addChild = function(childs) {
      //   var $parent = childs.parent();
      //   var num = childs.length;
      //
      //     for (var i = 0; i < num; i++) {
      //         $parent.append(childs.eq(i).clone(true));
      //     }
      //     $lis = $pics.find(".listpic li");
      // }
      // if(($lis.length < 5) && ($lis.length > 2)){
      //   $box.addChild($lis);
      // }


      //设置条数样式
      if (obj.auto) { //自动播放
        $pics.outbox = $box[0] ? $box : $pics; //自动播放的移进移除区域
        //定时器设置
        $pics.outbox.mouseleave(function(event) {
          clearInterval($pics.timer);
          $pics.timer = setInterval(function() {
            $pics.btnL.trigger('click');
          }, speed)
        }).mouseenter(function(event) {
          clearInterval($pics.timer)
        });
      }



      // 左右点击按钮
      $pics.btnL = $pics.find(".next");
      $pics.btnR = $pics.find(".pre");

      $pics.btnL.click(function() {

        if($pics.open==1){
          $pics.open=0;
          $pics.getLi($lis, -1)
          setTimeout(function(){
            $pics.open=1;
            ;
          },500)

        }else{return false;}
      });
      $pics.btnR.click(function() {
        if($pics.open==1){
          $pics.open=0;
          $pics.getLi($lis, 1);
          setTimeout(function(){
            $pics.open=1;

          },500)

        }else{return false;}

      });


      // 幻灯播放动画开始前的dom操作
      $pics.getLi = function(arr, num) {

        if (num == 1) {
          arr.last().insertBefore(arr.first());
        }
        if (num == -1) {
          arr.first().insertAfter(arr.last());
        }
        $lis = $pics.find(".listpic li");
        $pics.setClass($lis);

      }
      // 动画效果
      $pics.setClass = function(lis) {

        if (!$pics.animation) {
          $pics.animation = true;
        } else {
          $pics.addClass('animation');
          $small_list.addClass('animation');
        }
        for (var i = 0; i <= 5; i++) {
          lis.eq(i).attr("class", "li" + (i));
          if (i == 0 && $box[0]) {
            //底部小图片组切换
            $small_list.find("ul").removeClass('show');
            $small_list.find("ul").eq(lis.eq(i).attr("sort") - 1).addClass('show');
          }
        };


      }

      // 激活slide 默认值
      // $pics.btnR.trigger('click');

      //激活自动播放
      if (obj.auto) {
        $pics.outbox.trigger('mouseleave');
      }
    }




    $(document).ready(function() {

      //顶通关闭并删除
      l_adtop_1(".L_adtop_1");

      //下部幻灯和图文的组件
      le_slide_4({
        box: "#le_slide_4b", //模块div
        pics: ".le_slide_4", //幻灯部分
        small_list: ".list_commodity_4", //地下小图文部分
        auto: true, //自动播放
        speed: 5000 //间隔事件

      })



    });
    $(document).ready(function() {
      // 大数据js
      $('a[ref]').each(function(){
        var url  = $(this).attr('href');
        if(url){
          if(url.indexOf('?ref=')==-1 && url.indexOf('&ref=')==-1 ){
            var arr  = $(this).attr('href').split('?');
            var newurl = window.location.href;
            var action = $(this).attr('ref');
            if(arr[1] != undefined){
              newurl = url+'&ref='+action;
            }else{
              newurl = url+'?ref='+action;
            }
            $(this).attr('href',newurl);
          }
        }
      });
      // 点击事件的  统计
      $(document).on("click","[click-ref]",function(){
        var ref  = $(this).attr('click-ref');
        var action = $(this).attr('action');
        var category = $(this).attr('category');
        letvAlysSendAction(action, category, ref);
      });

    });
  </script>
