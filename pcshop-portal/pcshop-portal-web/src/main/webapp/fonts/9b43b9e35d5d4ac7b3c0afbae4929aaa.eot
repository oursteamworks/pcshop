//右侧飘窗显示隐藏方法
function showOrHide(parent,lis) {
				var $sidebar = $(parent);
				var $lis = $sidebar.find(lis);
				$sidebar.object="";

				$lis.mouseenter(function(event) {
					clearTimeout($sidebar.timer);
					$($sidebar.object).hide();
					$lis.removeClass('hover');
					$(this).addClass("hover");
					var id=$(this).find($(this).attr("object"));
					$(id).show();
				}).mouseleave(function(event) {
					$sidebar.object=$(this).attr("object");

					$sidebar.timer=setTimeout(function(){
						$lis.removeClass('hover');
						$($sidebar.object).hide()
					},300)
				});;
			}


